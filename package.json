{
  "name": "@venkatasudha/components",
  "version": "0.0.1",
  "description": "A production-ready, animated, and responsive components system for React/Next.js using Tailwind CSS and Framer Motion.",
  "author": "Rajesh Kanakamedala <rkanakamedala@venkatasudha.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,mjs,cjs,json,md,html,css,scss}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,mjs,cjs,json,md,html,css,scss}\"",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "commit": "cz",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "release:first": "standard-version --first-release"
  },
  "peerDependencies": {
    "framer-motion": ">=10.0.0",
    "lucide-react": ">=0.300.0",
    "next": ">=14.0.0",
    "next-themes": ">=0.3.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=3.0.0"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.0.7",
    "clsx": "^2.1.1",
    "focus-trap-react": "^10.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "commitizen": "^4.3.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.7",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.18.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "lint-staged": {
    "*.{json,md,html,css,scss}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "react",
    "nextjs",
    "navbar",
    "header",
    "tailwind",
    "tailwindcss",
    "framer-motion",
    "responsive",
    "animated",
    "component",
    "library",
    "venkatasudha"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanakamedala-rajesh/vs-components"
  },
  "bugs": {
    "url": "https://github.com/kanakamedala-rajesh/vs-components/issues"
  },
  "homepage": "https://github.com/kanakamedala-rajesh/vs-components#readme"
}
